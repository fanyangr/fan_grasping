<!-- finger 1 -->
	<link name="finger1-link0">
        <inertial>
            <origin xyz="0.03 0 0" rpy="0 0 0"/>
            <mass value="0.08"/>
            <inertia ixx="0.0001"  iyy="0.0001" izz="0.0002" ixy="0"  ixz="0" iyz="0"/>
        </inertial>
        <visual>
	        <origin xyz="0.03 0.0 0.0" rpy="0 0 0" />
