   <joint name="palm-j0" type="prismatic">
        <parent link="base_link" />
        <child link="virtual_link1" />
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <axis xyz="1 0 0" />
        <limit lower="-2.967" upper="2.967" effort="176" velocity="1.7104" />
    </joint>

    <joint name="palm-j1" type="prismatic">
        <parent link="virtual_link1" />
        <child link="virtual_link2" />
        <origin xyz="0 0 0" rpy="0 0 0"/>
